{
	"name": "@node-kit/extra.fs",
	"description": "Some shared extra utilities for nodejs build-in fs modules",
	"version": "2.4.0",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.cjs",
			"import": "./dist/*.mjs"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {},
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.34.4",
		"eslint": "^8.38.0",
		"prettier": "^2.8.7"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {}
		}
	},
	"keywords": [
		"node-kit",
		"extra",
		"extra.fs",
		"tools"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"homepage": "https://github.com/saqqdy/node-kit/tree/master/packages/extra/fs#readme",
	"bugs": {
		"url": "https://github.com/saqqdy/node-kit/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/node-kit.git",
		"directory": "packages/extra/fs"
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	}
}
