{
	"name": "@node-kit/which-pm",
	"description": "A simple utility to get which package manager used in the project",
	"version": "1.3.0-beta.1",
	"main": "./dist/index.cjs",
	"module": "./dist/index.mjs",
	"typings": "./dist/index.d.ts",
	"bin": {
		"which-pm": "./bin/index.mjs",
		"whichpm": "./bin/index.mjs"
	},
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.mjs"
		},
		"./*": {
			"require": "./dist/*.cjs",
			"import": "./dist/*.mjs"
		}
	},
	"scripts": {},
	"files": [
		"bin",
		"dist"
	],
	"directories": {
		"es": "es",
		"lib": "lib",
		"src": "src",
		"typings": "typings"
	},
	"dependencies": {
		"@node-kit/pm-info": "workspace:*"
	},
	"devDependencies": {
		"@microsoft/api-extractor": "^7.33.7",
		"eslint": "^8.31.0",
		"prettier": "^2.8.3"
	},
	"keywords": [
		"node-kit",
		"packageManager",
		"which-pm",
		"what-pm"
	],
	"license": "MIT",
	"author": "saqqdy <https://github.com/saqqdy>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/saqqdy/node-kit.git",
		"directory": "packages/which-pm"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"@babel/core",
				"webpack"
			],
			"allowedVersions": {}
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/saqqdy/node-kit/issues"
	},
	"homepage": "https://github.com/saqqdy/node-kit/tree/master/packages/which-pm#readme"
}
